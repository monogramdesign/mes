{
	"name": "svelte-ui",
	"version": "0.0.5",
	"private": true,
	"scripts": {
		"dev": "vite dev --port 3070",
		"build": "vite build",
		"postbuild": "node --loader ts-node/esm --experimental-specifier-resolution=node ./src/scripts/postbuild.ts",
		"package": "svelte-kit package",
		"preview": "vite preview --port 3000",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"dependencies": {
		"@fontsource/open-sans": "^4.5.13",
		"@prismicio/client": "^6.7.1",
		"@prismicio/helpers": "^2.3.5",
		"svelte-seo": "^1.4.1"
	},
	"devDependencies": {
		"@monogram/prettier-config": "^0.2.0",
		"@prismicio/types": "^0.2.3",
		"@sveltejs/adapter-vercel": "1.0.0-next.80",
		"@sveltejs/kit": "1.0.0-next.528",
		"@types/html-minifier": "^4.0.2",
		"@types/node": "*",
		"@types/prismic-dom": "^2.1.1",
		"@typescript-eslint/eslint-plugin": "^5.41.0",
		"@typescript-eslint/parser": "^5.41.0",
		"autoprefixer": "^10.4.13",
		"eslint": "^8.26.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"html-minifier": "^4.0.0",
		"postcss": "^8.4.18",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.8.0",
		"sass": "^1.55.0",
		"svelte": "^3.52.0",
		"svelte-check": "^2.9.2",
		"svelte-preprocess": "^4.10.7",
		"svelte-sitemap": "^2.5.2",
		"tailwindcss": "^3.2.1",
		"ts-node": "^10.9.1",
		"tslib": "^2.4.0",
		"typescript": "4.8.4",
		"vite": "^3.2.1"
	},
	"type": "module"
}
